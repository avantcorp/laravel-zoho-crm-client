{
    "name": "avantcorp/laravel-zoho-crm-client",
    "description": "A wrapper around avantcorp/laravel-zoho-client configured to connect to Zoho CRM",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Saleem",
            "email": "alisaleem@outlook.com"
        }
    ],
    "require": {
        "avantcorp/laravel-zoho-client": "^4.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Avant\\ZohoCRM\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "^10.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avant\\ZohoCRM\\ZohoCRMServiceProvider"
            ]
        }
    }
}
